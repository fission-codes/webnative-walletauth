{
  "name": "webnative-walletauth",
  "version": "0.1.0",
  "description": "Use the [Webnative SDK](https://github.com/fission-codes/webnative#readme) with a blockchain wallet. Access your personal encrypted file system with your wallet keys.",
  "scripts": {
    "build": "npm run build:lib && npm run build:bundle",
    "build:lib": "rimraf lib && tsc",
    "build:bundle": "rimraf dist && esbuild src/index.ts --bundle --format=esm --outfile=dist/webnative-walletauth.js --sourcemap",
    "prepare": "npm run build"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*",
    "./*": "./lib/*",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "dist",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fission-codes/webnative-walletauth.git"
  },
  "keywords": [
    "webnative",
    "walletauth",
    "wallet",
    "metamask",
    "ethereum"
  ],
  "author": "icidasset",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fission-codes/webnative-walletauth/issues"
  },
  "homepage": "https://github.com/fission-codes/webnative-walletauth#readme",
  "peerDependencies": {
    "webnative": "fission-codes/webnative#icidasset/state-refactor"
  },
  "dependencies": {
    "@noble/hashes": "^1.1.2",
    "@noble/secp256k1": "^1.6.3",
    "eip1193-provider": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "esbuild": "^0.15.7",
    "eslint": "^8.7.0",
    "events": "^3.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
